{
    "name": "chrismou/phergie-irc-plugin-react-pingpong",
    "description": "Phergie plugin for testing the bot's responsiveness",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "homepage": "http://github.com/chrismou/phergie-irc-plugin-react-pingpong",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Chris Chrisostomou",
            "email": "dev@mou.me",
            "homepage": "https://mou.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@mou.me",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/chrismou/phergie-irc-plugin-react-pingpong/issues",
        "source": "https://github.com/chrismou/phergie-irc-plugin-react-pingpong"
    },
    "require": {
        "php": ">=5.5.0",
        "phergie/phergie-irc-bot-react": "<3.0",
        "phergie/phergie-irc-plugin-react-command": "~2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "*",
        "vectorface/dunit": "^2.1",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\Phergie\\Plugin\\PingPong\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Phergie\\Tests\\Plugin\\PingPong\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
